    <Button
        android:id="@+id/cus"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="300dp"
        android:gravity="center"
        android:text="CUSTOMER"></Button>
</RelativeLayout>